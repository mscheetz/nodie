{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@now/static" },
    { "src": "package.json", "use": "@now/static-build" },
    { "src": "server.js", "use": "@now/node-server@0.4.26" }
  ],
  "routes": [
  {
    "src": "/example",
    "dest": "/index.html",
    "status": 200
  },
  {
  	"src": "/api/binance",
  	"dest": "/server.js",
  	"methods": ["GET"]
  }
]
}