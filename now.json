{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@now/static" },
    { "src": "*.js", "use": "@now/node@canary" }
  ],
  "routes": [
  {
    "src": "/example",
    "dest": "/index.html",
    "status": 200
  },
  {
  	"src": "/api/binance",
  	"dest": "/server.js",
  	"methods": ["GET"]
  }
]
}